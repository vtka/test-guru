<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to "view", test_path(test) %></td>
  <td><%= link_to "edit", edit_test_path(test) %></td>
  <td><%= link_to "delete", 
                  test_path(test), 
                  method: :delete,
                  data: { confirm: 'Are you sure?'} %></td>
  <td><%= button_to 'start', start_test_path(test) %></td>
</tr>
